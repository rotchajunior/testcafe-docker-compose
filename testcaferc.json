{
    "screenshots": {
        "path": "./screenshots",
        "takeOnFails": true,
        "pathPattern": "${DATE}_${TIME}/test-${TEST_INDEX}/${USERAGENT}/${FILE_INDEX}.png"
    },
    "quarantineMode": false,
    "debugMode": true,
    "debugOnFail": true,
    "stopOnFirstFail": true,
    "skipJsErrors": true,
    "selectorTimeout": 3000,
    "assertionTimeout": 10000,
    "pageLoadTimeout": 10000,
    "disablePageCaching": true
}